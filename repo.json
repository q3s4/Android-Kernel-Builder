        "binPath": ["./bin"]
      }
  ],
  "params": {
    "ARCH": "arm64",
    "CC": "clang13/clang/host/linux-x86/clang-r428724/bin/clang",
    "externalCommand": {}
  },
  "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Akitlove/AnyKernel3",
      "branch": "master"
  }
}
